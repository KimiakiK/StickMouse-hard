    (via_rule default)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND +5V +3V3 "Net-(D1-Pad2)" "LED_CONNECT" "Net-(F1-Pad2)" "USB_DP" "USB_DM"
    "JOYSTICK_V" "JOYSTICK_BTN" "JOYSTICK_H" SWDIO SWCLK NRST "Net-(R1-Pad1)" "BTN_LEFT"
    "BTN_RIGHT"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
